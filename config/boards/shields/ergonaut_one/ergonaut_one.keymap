/ {
        keymap {
                compatible = "zmk,keymap";
                default_layer {
			label = "main";
                        bindings = <
&none  &none         &w            &f NAV F  &p LALT P   &none  &none  &l          &u      &y             &none       &none
&none  &q            &r LMETA R    &s SYM S  &t RALT T   &none  &none  &n RALT N  &e       &o LMETA O     &z          &none
&none  &a            &x LCTRL X    &c MOU C  &v LSHFT V  &none  &none  &m LSHFT M  &comma  &dot LCTRL DOT &i          &none
&none &none &none &none &none &none
                        >;
                };

		symbol_layer {
			label = "symbol";
                        bindings = <
&trans   &trans   &trans   &trans   &trans   &trans   &none    &lpar   &rpar  &fslh  &none   &trans   
&trans   &trans   &trans   &trans   &trans   &trans   &none    &bspc   &ret   &space &star   &trans   
&trans   &trans   &trans   &trans   &trans   &trans   &none    &lss    &grt   &qt    &esc    &trans   
&none &none &none &none &none &none
                        >;
                };

		navigation_layer {
			label = "navigation";
                        bindings = <
&trans   &trans   &trans   &trans   &trans   &trans   &trans   &trans &up     &trans      &trans  &trans   
&trans   &trans   &trans   &trans   &trans   &trans   &trans   &left   &down   &right       &volup  &trans   
&trans   &trans   &trans   &trans   &trans   &trans   &trans   &trans  &trans  &trans   &voldn  &trans   
&none &none &none &none &none &none
                        >;
                };

                mouse_layer {
			label = "mouse";
                        bindings = <
&trans   &trans   &trans      &trans  &trans     &trans     &trans   &mtl          &mup    &mtr     &trans   &trans   
&trans   &trans   &trans      &trans  &trans     &trans     &trans   &mleft        &mdown  &mright  &trans   &trans   
&trans   &trans   &mt LALT X  &trans  &mbtn      &trans     &trans   &mbl LSHFT M  &mbc    &mbr     &trans   &trans   
